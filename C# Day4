using System;
using System.Diagnostics;
using System.Runtime;

namespace Simple
{
    class MyClass
    {
        public int Val = 20;
       
    }
    
    class Program
    {
        static void MyMethod(out MyClass f1,out int f2)
        {
            f1 = new MyClass();
            f1.Val = 25;
            f2 = 15;
        }
        
        static void Main()
        {
            MyClass a1 = null;
            int a2;

            MyMethod(out a1, out a2);
            Console.WriteLine($"{a1.Val},{a2}");
        }

       
    }
}
